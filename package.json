{
  "name": "contact-manager-fullstack",
  "version": "1.0.0",
  "description": "Full-stack contact manager application",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd contact-manager && npm run dev",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd contact-manager && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../contact-manager && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "author": "",
  "license": "ISC"
} 